#include<stdio.h>
int main()
{     
      int num1,num2,div;

      printf("Enter number 1");
      scanf("%d",&num1);
      printf("Enter number 2");
      scanf("%d",&num2);

      div=num1/num2;
      printf("The division of %d and %d is %d\t" ,num1,num2,div);

      return 0;
    
       





}
