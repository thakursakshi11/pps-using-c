#include<stdio.h>
int main()
{     
      int num1,num2,sub;

      printf("Enter number 1");
      scanf("%d",&num1);
      printf("Enter number 2");
      scanf("%d",&num2);

      sub=num1-num2;
      printf("The subtraction of %d and %d is %d\t" ,num1,num2,sub);

      return 0;
    
       





}
