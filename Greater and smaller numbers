#include <stdio.h>

int main()
{
    int num1, num2;

    
    printf("Enter the first number: ");
    scanf("%d", &num1);

    printf("Enter the second number: ");
    scanf("%d", &num2);

    
    if (num1 > num2)
    {
        printf("%d is greater and %d is smaller.\n", num1, num2);
    } 
    else if (num2 > num1)
    {
        printf("%d is greater and %d is smaller.\n", num2, num1);
    }  
    else 
    {
        printf("Both numbers are equal.\n");
    }

    return 0;
}
